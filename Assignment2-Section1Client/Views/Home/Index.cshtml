@model IEnumerable<Assignment2_Section1.Employee>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmpNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpFName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dept)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmpNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpFName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpLName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dept)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.EmpNo }) |
                @Html.ActionLink("Details", "Details", new { id= item.EmpNo }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.EmpNo })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
